<mujoco model="custom_env">
    <compiler angle="radian" inertiafromgeom="true"/>
    <option timestep="0.002"/>
    
    <worldbody>
        <!-- Add a table -->
        <body name="table" pos="0 0 0">
            <geom type="box" size="0.3 0.3 0.05" pos="0 0 0.025" rgba="0.8 0.8 0.8 1"/>
        </body>

        <!-- Add custom cameras -->
        <camera name="custom_camera1" pos="0.5 0.0 1.0" euler="0 -30 0" fovy="60"/>
        <camera name="custom_camera2" pos="-0.5 0.0 1.0" euler="0 -30 180" fovy="45"/>
    </worldbody>
</mujoco>

